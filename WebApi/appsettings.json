{

  "JwtSettings": {
    "Issuer": "https://localhost:44332/", //Web Api
    "Audience": "https://localhost:44392/", //MVC
    "SecretKey": "any string with 15+ chars", // 16 ou mais caracteres e não deve ficar no appsettings e sim no secrets
    "ExpiryDays": 1
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LibraryContext": "Server=(localdb)\\mssqllocaldb;Database=LibraryContext;Trusted_Connection=True;MultipleActiveResultSets=true",
    "LoginContext": "Server=(localdb)\\mssqllocaldb;Database=LoginContext;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}
