{
  "LibraryMusicalHttpOptions": {
    "Name": "LibraryHttpClient",
    "ApiBaseUrl": "https://localhost:44332/",
    "GroupPath": "api/Group",
    "AlbumPath":  "api/Album",
    "DayOut": 1
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LibraryMusicalContext": "Server=(localdb)\\mssqllocaldb;Database=LibraryMusicalDB;Trusted_Connection=True;MultipleActiveResultSets=true",
    "LoginContext": "Server=(localdb)\\mssqllocaldb;Database=LoginContext;Trusted_Connection=True;MultipleActiveResultSets=true"
  }
}